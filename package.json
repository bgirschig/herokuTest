{
  "name": "heroku_cordova_offline",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "engines": {
    "node": "6.11.1"
  },
  "scripts": {
    "start": "node server/server.js",
    "gulpBuild": "gulp",
    "cordovaBuild": "(cd cordovaWrap/ && ../node_modules/cordova/bin/cordova build android)",
    "cordovaRun": "(cd cordovaWrap/ && ../node_modules/cordova/bin/cordova run android)",
    "fullBuild": "npm test && npm run gulpBuild && npm run cordovaBuild",
    "fullRun": "npm test && npm run gulpBuild && npm run cordovaRun",
    "test": "mocha tests/*/*.js",
    "clientTest": "mocha tests/client/*.js",
    "serverTest": "mocha tests/server/*.js",
    "deployServer": "npm run serverTest && git push heroku master"
  },
  "author": "Bastien Girschig <bastien.girschig@gmail.com> (http://www.bastiengirschig.fr)",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.15.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "cordova": "^7.0.1",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "mocha": "^3.4.2",
    "node-fetch": "^1.7.1",
    "uniq": "^1.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
